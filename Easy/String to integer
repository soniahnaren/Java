public class stringtonteger {
    public static void main(String[] args) {
        String input = "1234";
        int output = 0;
        int length = input.length();

        for (int i = 0; i < length; i++) {
            char c = input.charAt(i);
            int digit = c - '0';
            output = output * 10 + digit;
        }
        System.out.println("Output Integer: " + output);
    }
}
